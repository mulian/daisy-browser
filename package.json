{
  "name"    : "daisy-browser",
  "description": "A Browser for Daisy Web-App. Used with an electronic Blackboard.",
  "version" : "0.1.0",
  "main"    : "main.js",
  "engines": {
    "electron-prebuilt": ">=0.33.3"
  },
  "devDependencies": {
    "docco": ">=0.7.0"
  },
  "dependencies": {
    "favicon-and-title": "git+https://github.com/mulian/node-favicon-and-title.git#0605a30",
    "coffee-script":">=1.10.0",
    "less": ">=2.5.3",
    "jquery": ">=2.1.4"
  },
  "flash": {
    "path": "/Applications/Google Chrome.app/Contents/Versions/45.0.2454.101/Google Chrome Framework.framework/Internet Plug-Ins/PepperFlash/PepperFlashPlayer.plugin",
    "version": "19.0.0.185"
  },
  "settings": {
    "url":"http://localhost:8081/",
    "guesture": {
      "space": 20,
      "minActivate": 40
    },
    "showIntroduction": true,
    "dirUpload": {
      "enabled": true,
      "dir": "/Users/maxh/Desktop"
    },
    "nativeApps" : {
      "visio" : "/path/to/Visio",
      "calc" : "/path/calc"
    }
  },
  "scripts": {
    "start": "electron ./",
    "doc": "docco ./lib/*.coffee"
  }
}
